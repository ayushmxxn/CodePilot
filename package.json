{
  "name": "codepilot",
  "displayName": "CodePilot",
  "description": "Instant access to documentations, frameworks and libraries right within VS Code",
  "version": "1.0.0",
  "publisher": "AyushmaanSingh",
  "icon": "icon.jpeg",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ayushmaan0101/CodePilot"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Documentation",
    "Frameworks",
    "Libraries",
    "CODEPILOT",
    "CodePilot",
    "Codepilot",
    "codepilot",
    "code pilot"
  ],
  "activationEvents": [
    "onCommand:extension.openDocumentation"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.openDocumentation",
        "title": "CodePilot"
      }
    ]
  },
  "keybindings": [
    {
      "command": "extension.openDocumentation",
      "key": "ctrl+shift+p",
      "mac": "cmd+shift+p",
      "when": "editorTextFocus"
    }
  ],
  "main": "./out/extension.js",
  "webview": {
    "retainContextWhenHidden": true
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "typescript": "^4.5.5",
    "vscode": "^1.1.37"
  },
  "dependencies": {},
  "private": true
}
